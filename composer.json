{
    "name": "bag2/cookie",
    "description": "Cookie functions over PSR-7 and setcookie() function.",
    "license": "MPL-2.0",
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "badoo/soft-mocks": "^3.0",
        "nyholm/psr7": "^1.2",
        "php-http/psr7-integration-tests": "^1.0",
        "phpunit/phpunit": "^7",
        "vaimo/composer-patches": "^4.20"
    },
    "autoload": {
        "psr-4": {
            "Bag2\\Cookie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bag2\\Cookie\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
